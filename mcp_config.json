{
  "mcp-server": {
    "name": "mcp-server",
    "version": "0.1.0",
    "description": "MCP Server with Gemini AI integration",
    "server": {
      "host": "0.0.0.0",
      "port": 8005,
      "reload": true,
      "workers": 1,
      "log_level": "info",
      "cors": {
        "allow_origins": ["http://localhost:8005"],
        "allow_credentials": true,
        "allow_methods": ["*"],
        "allow_headers": ["*"]
      }
    },
    "gemini": {
      "api_key": "${GEMINI_API_KEY}",
      "default_model": "gemini-1.5-flash",
      "default_temperature": 0.7,
      "max_tokens": 2048
    }
  },
  "mcp-demo": {
    "name": "mcp-demo",
    "version": "0.1.0",
    "description": "MCP Demo Server",
    "server": {
      "host": "0.0.0.0",
      "port": 8002,
      "reload": true,
      "workers": 1,
      "log_level": "debug",
      "cors": {
        "allow_origins": ["http://localhost:8002"],
        "allow_credentials": true,
        "allow_methods": ["*"],
        "allow_headers": ["*"]
      }
    },
    "endpoints": {
      "base_url": "http://localhost:8002",
      "api_prefix": "/api",
      "docs_url": "/docs",
      "openapi_url": "/openapi.json"
    }
  }
}
